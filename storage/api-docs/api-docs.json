                "operationId": "42e38e1ec9a77688ed78f16868c19a31",
                "parameters": [
                    {
                        "name": "nombre",
                        "in": "query",
                        "description": "Nombre del hotel para filtrar",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "direccion",
                        "in": "query",
                        "description": "Dirección del hotel para filtrar",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "telefono",
                        "in": "query",
                        "description": "Teléfono del hotel para filtrar",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "email",
                        "in": "query",
                        "description": "Email del hotel para filtrar",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "sitioWeb",
                        "in": "query",
                        "description": "Sitio web del hotel para filtrar",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "per_page",
                        "in": "query",
                        "description": "Cantidad de elementos por página",
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "name": "page",
                        "in": "query",
                        "description": "Número de página",
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "name": "includeHabitaciones",
                        "in": "query",
                        "description": "Inluye el numero de habitaciones",
                        "schema": {
                            "type": "boolean"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Lista de hoteles filtrada y paginada"
                    }
                }
            },
